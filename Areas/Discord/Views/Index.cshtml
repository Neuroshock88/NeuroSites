@model NeuroSites.Areas.Discord.Default
@{
    NeuroSites.Areas.Discord.Default NewModel = NeuroSites.Models.PagesModel.ActiveModel;
}
@if (NewModel != null)
{
    <div class="discord" id="discord">
        <div class="discord__logo">
            <a href="https://discord.gg/kucbPAg" target="_blank" class="discord__logo-img"><img src="~/Content/Images/Logos/discord.png" /></a>
            <a href="https://discord.gg/kucbPAg" target="_blank" class="discord__logo-text">Join Neuroshock88's official Discord Server!!!</a>
        </div>
        <div class="discord__text-content">
            <iframe src="https://discordapp.com/widget?id=701928525388578836&theme=dark" width="350" height="500" allowtransparency="true" frameborder="0" sandbox="allow-popups allow-popups-to-escape-sandbox allow-same-origin allow-scripts"></iframe>
        </div>
    </div>

}